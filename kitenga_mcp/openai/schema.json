{
    "name": "openai",
    "version": "1.0.0",
    "description": "OpenAI Responses API integration schema.",
    "baseUrl": "https://tiwhanawhana-backend.onrender.com/api/v1/openai",
    "authentication": {
        "type": "bearer",
        "tokenEnv": "OPENAI_API_KEY",
        "required": true
    },
    "services": {
        "responses": {
            "description": "Response generation and retrieval",
            "endpoints": [
                "/responses",
                "/responses/{response_id}"
            ]
        }
    }
}