{
    "name": "supabase",
    "version": "1.0.0",
    "description": "Supabase Database Service Schema",
    "baseUrl": "${SUPABASE_URL}/rest/v1",
    "authentication": {
        "type": "bearer",
        "tokenEnv": "SUPABASE_KEY",
        "required": true
    },
    "services": {
        "tables": {
            "description": "Database table operations",
            "endpoints": [
                "/rpc/{function_name}"
            ]
        },
        "auth": {
            "description": "Authentication management",
            "endpoints": [
                "/auth/v1/user",
                "/auth/v1/users"
            ]
        },
        "storage": {
            "description": "File storage operations",
            "endpoints": [
                "/storage/v1/buckets"
            ]
        }
    },
    "inputSchema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
            "table": {
                "type": "string",
                "description": "Table name"
            },
            "query": {
                "type": "object",
                "description": "Query filters and operations"
            },
            "data": {
                "type": "object",
                "description": "Row data for insert/update"
            },
            "filters": {
                "type": "object",
                "description": "WHERE clause conditions"
            }
        }
    },
    "outputSchema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
            "status": {
                "type": "integer"
            },
            "data": {
                "type": "array"
            },
            "error": {
                "type": "string"
            }
        }
    }
}