    "/chat/memory-ingest": {
      "post": {
        "tags": [
          "Chat"
        ],
        "summary": "Ingest chat text for memory",
        "description": "Save chat text as raw content, run it through the stealth pipeline, and store vector metadata for short-term recall.",
        "operationId": "chat_memory_ingest",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "session_id": {
                    "type": "string",
                    "description": "Unique chat session id."
                  },
                  "text": {
                    "type": "string",
                    "description": "Text payload to save and vectorize."
                  },
                  "role": {
                    "type": "string",
                    "description": "Contributor identity (user or assistant).",
                    "default": "user"
                  },
                  "thread_id": {
                    "type": "string",
                    "description": "Optional thread identifier."
                  },
                  "assistant_reply": {
                    "type": "string",
                    "description": "Optional assistant response accompanying the user text."
                  }
                },
                "required": [
                  "session_id",
                  "text"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Chat text saved + pipeline metadata",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {"type": "string"},
                    "session_id": {"type": "string"},
                    "role": {"type": "string"},
                    "vector_batch_id": {"type": "string"},
                    "pipeline_metadata": {"type": "object", "additionalProperties": true}
                  }
                }
              }
            }
          }
        }
      }
    }
